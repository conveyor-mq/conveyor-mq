  "description": "A fast, robust and extensible distributed task/job queue for Node.js",
  "scripts": {
    "clean": "rm -rf ./dist",
    "build": "tsc && cp -r ./src/lua/*.lua ./dist/lua/",
    "test": "jest --runInBand --forceExit ./src",
    "test:watch": "jest --runInBand --watch ./src",
    "start": "ts-node ./src/index.ts",
    "lint": "eslint ./src/**/*.ts",
    "shell": "ts-node"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jasrusable/conveyor.git"
  },
  "author": "Jason Russell",
  "license": "ISC",
  "keywords": [
    "distributed",
    "queue",
    "task-queue",
    "tasks",
    "job-queue",
    "jobs",
    "worker",
    "worker-queue",
    "redis"
  ],
  "bugs": {
    "url": "https://github.com/jasrusable/conveyor/issues"
  },
  "homepage": "https://github.com/jasrusable/conveyor#readme",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "dependencies": {
    "@types/ioredis": "^4.16.1",
    "@types/lodash": "^4.14.150",
    "@types/p-timeout": "^1.2.2",
    "@types/set-interval-async": "^1.0.0",
    "@types/uuid": "^7.0.3",
    "ioredis": "^4.16.3",
    "lodash": "^4.17.15",
    "moment": "^2.25.3",
    "p-queue": "^6.4.0",
    "p-timeout": "^3.2.0",
    "set-interval-async": "^1.0.32",
    "uuid": "^8.0.0"
  },
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "@typescript-eslint/eslint-plugin": "^2.31.0",
    "@typescript-eslint/parser": "^2.31.0",
    "coveralls": "^3.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.3",
    "jest": "^26.0.1",
    "prettier": "^2.0.5",
