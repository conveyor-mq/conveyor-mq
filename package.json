{
  "name": "conveyor-mq",
  "version": "0.0.38",
  "description": "A fast, robust and extensible distributed task/job queue for Node.js",
  "scripts": {
    "clean": "rm -rf ./dist",
    "build": "npm run clean && tsc -b && copyfiles --up 1 src/lua/*.lua ./dist/",
    "build-docs": "typedoc --theme ./node_modules/typedoc-neo-theme/bin/default",
    "test": "jest --runInBand --forceExit ./src",
    "test:watch": "jest --runInBand --watch ./src",
    "start": "ts-node ./src/index.ts",
    "lint": "eslint ./src/**/*.ts",
    "shell": "ts-node"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/conveyor-mq/conveyor-mq.git"
  },
  "author": "Jason Russell",
  "license": "MIT",
  "keywords": [
    "distributed",
    "queue",
    "task-queue",
    "tasks",
    "job-queue",
    "jobs",
    "worker",
    "worker-queue",
    "redis"
  ],
  "bugs": {
    "url": "https://github.com/conveyor-mq/conveyor-mq/issues"
  },
  "homepage": "https://github.com/conveyor-mq/conveyor-mq#readme",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "dependencies": {
    "date-fns": "^2.29.3",
    "debug": "^4.3.4",
    "ioredis": "^5.2.3",
    "p-queue": "^6.6.2",
    "p-timeout": "^4.1.0",
    "rate-limiter-flexible": "^2.4.0",
    "set-interval-async": "^3.0.2",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/debug": "4.1.7",
    "@types/jest": "29.2.0",
    "@types/node": "18.11.11",
    "@types/uuid": "8.3.4",
    "@typescript-eslint/eslint-plugin": "5.40.1",
    "@typescript-eslint/parser": "5.40.1",
    "copyfiles": "2.4.1",
    "coveralls": "3.1.1",
    "eslint": "8.26.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "3.5.2",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.2.1",
    "jest": "28.1.3",
    "np": "7.6.2",
    "prettier": "2.7.1",
    "ts-jest": "28.0.8",
    "ts-node": "10.9.1",
    "typedoc": "0.23.17",
    "typedoc-neo-theme": "1.1.1",
    "typescript": "4.8.4"
  }
}
