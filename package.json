{
  "name": "conveyor-mq",
  "version": "0.0.26",
  "description": "A fast, robust and extensible distributed task/job queue for Node.js",
  "scripts": {
    "clean": "rm -rf ./dist",
    "build": "npm run clean && tsc && cp -r ./src/lua/*.lua ./dist/lua/",
    "build-docs": "typedoc --theme ./node_modules/typedoc-neo-theme/bin/default",
    "publish": "np",
    "test": "jest --runInBand --forceExit ./src",
    "test:watch": "jest --runInBand --watch ./src",
    "start": "ts-node ./src/index.ts",
    "lint": "eslint ./src/**/*.ts",
    "shell": "ts-node"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/conveyor-mq/conveyor-mq.git"
  },
  "author": "Jason Russell",
  "license": "ISC",
  "keywords": [
    "distributed",
    "queue",
    "task-queue",
    "tasks",
    "job-queue",
    "jobs",
    "worker",
    "worker-queue",
    "redis"
  ],
  "bugs": {
    "url": "https://github.com/conveyor-mq/conveyor-mq/issues"
  },
  "homepage": "https://github.com/conveyor-mq/conveyor-mq#readme",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "dependencies": {
    "@types/debug": "^4.1.5",
    "@types/ioredis": "^4.16.4",
    "@types/lodash": "^4.14.155",
    "@types/set-interval-async": "^1.0.0",
    "@types/uuid": "^8.0.0",
    "debug": "^4.1.1",
    "ioredis": "^4.17.3",
    "lodash": "^4.17.15",
    "moment": "^2.26.0",
    "p-queue": "^6.4.0",
    "p-timeout": "^3.2.0",
    "set-interval-async": "^1.0.32",
    "uuid": "^8.1.0"
  },
  "devDependencies": {
    "@types/jest": "^25.2.3",
    "@typescript-eslint/eslint-plugin": "^3.1.0",
    "@typescript-eslint/parser": "^3.1.0",
    "coveralls": "^3.1.0",
    "eslint": "^7.1.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.3",
    "jest": "^26.0.1",
    "np": "^6.2.3",
    "prettier": "^2.0.5",
    "ts-jest": "^26.1.0",
    "ts-node": "^8.10.2",
    "typedoc": "^0.17.7",
    "typedoc-neo-theme": "^1.0.8",
    "typescript": "^3.9.5"
  }
}
